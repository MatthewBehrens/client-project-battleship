<h2>place your boats</h2>

<section class="ocean-view">

<div class="grid">

<% 0.upto 9 do |row| %>

<div class="row r_<%= row %>" >

    <% 0.upto 9 do |column| %>
        <div class="col c_<%= column %>" >
          <div class="cell" id="cell_<%= row %><%= column %>">

          </div>
        </div>
    <% end %>

</div>

<% end %>

</div>

</section>

<section class="boat-dock">

<% @boats.each do | boat | %>
<div class="boat-container"><h3><%= boat %></h3></div>
<% end %>

</section>